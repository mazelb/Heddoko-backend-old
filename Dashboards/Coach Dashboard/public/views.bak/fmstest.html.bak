<header class="page-main-header">
    <div class="row">
        <div class="col-sm-6">
            <h4>Submit FMS Results</h4>
            <p>Select an athlete and use the form below to add FMS results</p>
        </div>
    </div>
</header>

<div class="page page-dashboard" data-ng-controller="FMSFormController">

	<section>
		<div class="row">
			<div class="col-sm-12">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="text-left col-sm-6">
							<h3>
								Athlete
							</h3>

							<select ng-model="data.member.selected"
									ng-selected="data.member.selected"
									ng-options="member.first_name for member in data.member.list track by member.id" >
							</select>
						</div>

						<div class="panel-body col-sm-8 ng-scope">


						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<section>
		<div class="row">

			<div class="col-sm-12" ng-hide="waiting_server_response">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="text-left col-sm-6">
							<h3>
								Add FMS Test Scores
							</h3>
						</div>

						<div class="panel-body col-sm-12 ng-scope">
							<div ng-show="data.member.selected.id < 1">
                                Select an athlete under 'Dashboard' in the sidebar before submitting an FMS Form
                            </div>

							<form ng-submit="submitFMSForm()" ng-hide="data.member.selected.id < 1">
								<div class="row">
									<div class="col-sm-3">
										Deep Squat
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.deepsquat"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.deepsquat"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.deepsquat"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.deepsquat"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.deepsquatcomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.deepsquat_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Hurdle step (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Lhurdle"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Lhurdle"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Lhurdle"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Lhurdle"><span>3</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Lhurdle_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Hurdle step (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Rhurdle"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Rhurdle"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Rhurdle"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Rhurdle"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.hurdlecomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Rhurdle_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Inline lunge (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Llunge"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Llunge"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Llunge"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Llunge"><span>3</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Llunge_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Inline lunge (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Rlunge"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Rlunge"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Rlunge"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Rlunge"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.lungecomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Rlunge_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Shoulder Mobility (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Lshoulder"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Lshoulder"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Lshoulder"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Lshoulder"><span>3</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Lshoulder_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Shoulder Mobility (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Rshoulder"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Rshoulder"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Rshoulder"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Rshoulder"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.shouldercomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Rshoulder_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Impingement clearing test (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-checkbox"><input ng-model="data.fms_form_data.Limpingement" type="checkbox"><span>pain</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Limpingement_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Impingement clearing test (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-checkbox"><input ng-model="data.fms_form_data.Rimpingement" type="checkbox"><span>pain</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.impingementcomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Rimpingement_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Active straight-leg raise (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Lactive"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Lactive"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Lactive"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Lactive"><span>3</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Lactive_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Active straight-leg raise (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Ractive"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Ractive"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Ractive"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Ractive"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.activecomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Ractive_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Trunk stability push-up
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.trunk"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.trunk"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.trunk"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.trunk"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.trunkcomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.trunk_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Press-up clearing test
									</div>
									<div class="col-sm-3">
										<label class="ui-checkbox"><input ng-model="data.fms_form_data.press" type="checkbox"><span>pain</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.presscomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.press_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Rotary stability (L)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Lrotary"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Lrotary"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Lrotary"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Lrotary"><span>3</span></label>
									</div>
									<div class="col-sm-3">

									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Lrotary_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Rotary stability (R)
									</div>
									<div class="col-sm-3">
										<label class="ui-radio"><input type="radio" value="0" ng-model="data.fms_form_data.Rrotary"><span>0</span></label>
										<label class="ui-radio"><input type="radio" value="1" ng-model="data.fms_form_data.Rrotary"><span>1</span></label>
										<label class="ui-radio"><input type="radio" value="2" ng-model="data.fms_form_data.Rrotary"><span>2</span></label>
										<label class="ui-radio"><input type="radio" value="3" ng-model="data.fms_form_data.Rrotary"><span>3</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.rotarycomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.Rrotary_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-3">
										Posterior rocking clearing test
									</div>
									<div class="col-sm-3">
										<label class="ui-checkbox"><input ng-model="data.fms_form_data.posterior" type="checkbox"><span>pain</span></label>
									</div>
									<div class="col-sm-3">
										<input type="text" ng-model="data.fms_form_data.posteriorcomments">
									</div>
									<div class="col-sm-3">
										<input type="file" title="Upload Movement" data-ui-file-upload="" class="btn-success" file-model="data.fms_form_movement_files.posterior_movement_file">
									</div>
								</div>
								<div class="row">
									<div class="col-sm-6">
									</div>
									<div class="col-sm-6">
										<h3>comments</h3>
										<textarea name="" id="" class="form-control" ng-model="data.fms_form_data.comment"></textarea>
										<br/>
										<div class="form-group text-right">
											<button type="submit" class="btn btn-primary btn-lg">Submit Test</br>Results</button>
										</div>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
</div>
